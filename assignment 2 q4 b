#include <iostream>
using namespace std;

int main() {
    char string[100];
    cout << "Enter a string: ";
    cin.getline(string, 100);

    int len = strlen(string);
    cout << "Reversed string: ";
    for(int i = len - 1; i >= 0; i--) {
        cout << string[i];
    }
    cout << endl;

    return 0;
}