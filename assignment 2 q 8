#include <iostream>
#include <unordered_set>
using namespace std;

int main() {
    int n;
    cin >> n;
    int *arr = new int[n];
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    unordered_set<int> distinctElements;
    for (int i = 0; i < n; i++)
        distinctElements.insert(arr[i]);

    cout << distinctElements.size() << "\n";

    delete[] arr;
    return 0;
}
