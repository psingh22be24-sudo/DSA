#include <iostream>
#include <cstring>
using namespace std;

int main() {
    int n;
    cout << "Enter number of strings: ";
    cin >> n;
    cin.ignore();

    char str[100][100];

    // Input strings
    for(int i = 0; i < n; i++) {
        cout << "Enter string " << i + 1 << ": ";
        cin.getline(str[i], 100);
    }

    // Bubble sort
    char temp[100];
    for(int i = 0; i < n - 1; i++) {
        for(int j = i + 1; j < n; j++) {
            if (strcmp(str[i], str[j]) > 0) {
                strcpy(temp, str[i]);
                strcpy(str[i], str[j]);
                strcpy(str[j], temp);
            }
        }
    }

    // Output sorted strings
    cout << "Strings in alphabetical order:\n";
    for(int i = 0; i < n; i++) {
        cout << str[i] << endl;
    }

    return 0;
}